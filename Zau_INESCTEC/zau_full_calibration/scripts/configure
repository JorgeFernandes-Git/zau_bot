#!/bin/sh
# Auto generated on 28/04/2023 12:12:19
#      ATOMIC Framework

helpFunction()
{
   echo ""
   echo "Usage: $0 -c config_file.yml"
   echo -e "	-c Name of specific configuration file that must be inside the package_calibration/calibration/ folder."
   exit 1
}
while getopts "c:h" flag
do
    case "${flag}" in
        c ) configfile="$OPTARG" ;;
        h ) helpFunction;;
    esac
done

if test -z "$configfile"
then
rosrun atom_calibration configure_calibration_pkg -n zau_full_calibration
else
rosrun atom_calibration configure_calibration_pkg -n zau_full_calibration -cfg $configfile
fi